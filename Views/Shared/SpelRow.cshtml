@model ReversiMvcApp.Models.Spel
<tr>
    <td>
        @Html.DisplayFor(m => m.Omschrijving)
    </td>
    <td>
        <form asp-action="Join">
            <div class="form-group">
                <input asp-for="Token" type="hidden" />
            </div>
            <div class="form-group">
                <input type="submit" value="Join" class="btn btn-primary" />
            </div>
        </form>
    </td>
</tr>